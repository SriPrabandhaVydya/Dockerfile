pipeline {
  agent any
  stages {
    stage ('Build') {
      steps {
        bat 'printenv'
        bat 'docker build -t sriprabandhavydya/Dockerfile:"%GIT_COMMIT%" .'
      }
    }
    
    stage ('Publish to DockerHub') {
      steps {
        withDockerRegistry([credentialsId: "docker-hub", url: ""]) {
          bat 'docker push sriprabandhavydya/Dockerfile:"%GIT_COMMIT%"'
        }
      }
    }
  }
}
                   
